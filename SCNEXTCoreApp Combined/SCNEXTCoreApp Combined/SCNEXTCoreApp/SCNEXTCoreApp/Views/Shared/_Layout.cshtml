    <style>
        body {
            font-family: "Lato", sans-serif;
        }

        .sidenav {
            height: 100%;
            width: 300px;
            position: fixed;
            z-index: 1;
            top: 0;
            left: 0;
            background: none;
            overflow-x: hidden;
            padding-top: 20px;
        }
        
            /* Style the sidenav links and the dropdown button */
            .sidenav a, .dropdown-btn {
                padding: 6px 8px 6px 16px;
                text-decoration: none;
                font-size: 20px;
                color: #818181;
                display: block;
                border: none;
                background: none;
                width: 100%;
                text-align: left;
                cursor: pointer;
                outline: none;
            }

                /* On mouse-over */
                .sidenav a:hover, .dropdown-btn:hover {
                    color: #f1f1f1;
                }

        /* Main content */
        .container {
            margin-left: 300px; /* Same as the width of the sidenav */
            font-size: 20px; /* Increased text to enable scrolling */
            padding: 0px 10px;
        }

 .vertical-center {
  margin: 0;
  position: absolute;
  //top: 50%;
   left: 50%;
  -ms-transform: translateX(-50%);
  transform: translateX(-50%);
}
 .center {
  margin: 0;
  position: absolute;
  top: 30%;
  left: 50%;
  -ms-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
}
        .body-content {
             margin-left: 300px; 
             padding-top: 50px;  /* choose whatever you want here */
             // padding-right: 200px;
}
        /* Add an active class to the active dropdown button */
        .active {
            background-color: green;
            color: white;
        }
    </style>
